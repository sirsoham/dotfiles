"*":
  core:
    themes: [
      "atom-material-ui"
      "base16-tomorrow-dark-theme"
    ]
  editor:
    fontSize: 12
    invisibles: {}
    tabLength: 4
  "exception-reporting":
    userId: "9b671e90-aca1-3b96-cdf2-ba608de89cf6"
  "highlight-selected": {}
  linter: {}
  "linter-flake8":
    maxLineLength: 80
  minimap:
